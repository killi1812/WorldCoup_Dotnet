<UserControl xmlns:WpfGui="clr-namespace:WpfGui"  x:Class="WpfApp.TactisLayout._4Row"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WpfApp.TactisLayout"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="150">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
            <RowDefinition Height="Auto" />
            <RowDefinition Height="*" />
            <RowDefinition Height="Auto" />
            <RowDefinition Height="*" />
            <RowDefinition Height="Auto" />
            <RowDefinition Height="*" />
            <RowDefinition Height="Auto" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <WpfGui:PlayerProfile HorizontalAlignment="Center" Grid.Row="1" />
        <WpfGui:PlayerProfile HorizontalAlignment="Center" Grid.Row="3"/>
        <WpfGui:PlayerProfile HorizontalAlignment="Center" Grid.Row="5"/>
        <WpfGui:PlayerProfile HorizontalAlignment="Center" Grid.Row="7"/>
    </Grid>
</UserControl>
