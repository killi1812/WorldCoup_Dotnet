<Window x:Class="WpfGui.SettingsWinfow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfGui"
        mc:Ignorable="d"
        Title="SettingsWinfow" Height="200" Width="400
        ">
    <Grid Margin="0,0,0,0
          ">
        <Button Content="Ok" HorizontalAlignment="Left" Margin="120,125,0,0" VerticalAlignment="Top" Width="50"/>
        <Button Content="Cancle" HorizontalAlignment="Left" Margin="220,123,0,0" VerticalAlignment="Top" Width="50"/>
        <StackPanel Margin="50,40,250,100">
            <RadioButton Content="en" Margin="5"/>
            <RadioButton Content="hr" Margin="5"/>

        </StackPanel>
        <StackPanel Margin="240,40,50,100">
            <RadioButton Content="Man" Margin="5"/>
            <RadioButton Content="Woman" Margin="5"/>

        </StackPanel>
    </Grid>
</Window>
